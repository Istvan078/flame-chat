{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html?version=2",
          "/manifest.webmanifest",
          "/*.css?version=2",
          "/*.js?version=2"
        ],
        "urls": [
          "https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-call",
      "urls": ["https://flamechat07.web.app/**"],
      "cacheConfig": {
        "maxAge": "10h",
        "maxSize": 20,
        "strategy": "freshness",
        "timeout": "5s"
      }
    },
    {
      "name": "firebase-images",
      "urls": [
        "https://firebasestorage.googleapis.com/v0/b/flamechat07.firebasestorage.app/o/**",
        "https://firebasestorage.googleapis.com/v0/b/project0781.appspot.com/o/**",
        "https://lh3.googleusercontent.com/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxAge": "30d",
        "maxSize": 500,
        "cacheOpaqueResponses": true
      },
      "cacheQueryOptions": { "ignoreSearch": false }
    }
  ]
}
